<h1>
	<%= icon(@object.class.inki_icon) -%>
	<% if defined? @object.title -%>
    <%= @object.title  %>
  <% else -%>
    <% t(controller_name) %>
  <% end -%>
</h1>
<div class="row">
	<div class="col-md-12">
		<%= back_link(@object) %>
		<%= render :partial => "layouts/show_options" %>
	</div>
</div>
<div class="row">
	<div class="col-md-12">
		<div id="<%= @div_id %>">
		</div>
	</div>
</div>
<div class="row">
	<div class="col-md-12">
		<%= render :partial => "layouts/show_table", :locals => {:non_dropdown_view => true} %>
	</div>
</div>
